        </aside>
        <div class="content-wrapper">
            <section class="content">
                <div class="row">
                    @RenderBody()
                </div>
            </section>
        </div>
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> 2.2.0
            </div>
            <strong>Copyright &copy; 2020 <a title="@Commsights.Data.Helpers.AppGlobal.Title" target="_blank" href="https://www.commsights.com/">@Commsights.Data.Helpers.AppGlobal.Title</a>.</strong> All rights reserved.
        </footer>
        <div class="control-sidebar-bg"></div>
    </div>
    <script>
        function setMenuLeftActive() {
            var tag = $('a[href="' + this.location.pathname + '"]');
            var tag02 = tag.parent('li');
            tag02.addClass('active');
            var tag01 = tag.parent('li').parent('ul').parent('li');
            tag01.addClass('active');
        }
        jQuery(function ($) {
            setMenuLeftActive();
        });
    </script>
    <script src="/dist/js/app.min.js" type="text/javascript"></script>
    <script src="/js/site.js" type="text/javascript"></script>
</body>
