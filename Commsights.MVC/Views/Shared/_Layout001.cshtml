    <link href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css" rel="stylesheet" type="text/css" />
    <link href="https://kendo.cdn.telerik.com/2020.2.617/styles/kendo.bootstrap-v4.min.css" rel="stylesheet" type="text/css" />
    <script src="https://kendo.cdn.telerik.com/2020.2.617/js/jquery.min.js"></script>
    <script src="https://kendo.cdn.telerik.com/2020.2.617/js/jszip.min.js"></script>
    <script src="https://kendo.cdn.telerik.com/2020.2.617/js/kendo.all.min.js"></script>
    <script src="https://kendo.cdn.telerik.com/2020.2.617/js/kendo.aspnetmvc.min.js"></script>
    <script>
        function BeginRequestHandler() {
            Slider();
        }
        function EndRequestHandler() {
            $('#Dim').css("display", "none");
            $('#BackGround').fadeTo("fast", 2);
        }
        function Slider() {
            $('#Dim').css("display", "");
            $('#Dim').css("z-index", "10");
            $('#Dim').css("position", "absolute");
            $('#Dim').css("top", ($(window).height() - $('#Dim').height()) / 2 + $(window).scrollTop() + "px");
            $('#Dim').css("left", ($(window).width() - $('#Dim').width()) / 2 + $(window).scrollLeft() + "px");
            $('#BackGround').fadeTo("fast", 0.5);
            $('#Dim').focus();
        }
    </script>
</head>
<body class="skin-blue sidebar-mini" id="BackGround">
    <div id="Dim" style="display: none;">
        <div class="msgbox">
            <img src="@Commsights.Data.Helpers.AppGlobal.LoadingURLFull" alt="Processing, please wait!"
                 title="Processing, please wait!" width="200px" height="200px" />
        </div>
    </div>
    <div class="wrapper">
        <header class="main-header">
            <a href="@Commsights.Data.Helpers.AppGlobal.Domain" class="logo">
                <span class="logo-mini"><b>CRM</b></span>
                <span class="logo-lg"><b>@Commsights.Data.Helpers.AppGlobal.Title - CRM</b></span>
            </a>
            <nav class="navbar navbar-static-top" role="navigation">
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <li class="dropdown user user-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <img src="@Commsights.Data.Helpers.AppGlobal.LogoURLFull" class="user-image" alt="Lê Thanh Sơn" />
                                <span class="hidden-xs">Lê Thanh Sơn</span>
                            </a>
                            <ul class="dropdown-menu">
                                <!-- User image -->
                                <li class="user-header">
                                    <img src="@Commsights.Data.Helpers.AppGlobal.LogoURLFull" class="img-circle" alt="Lê Thanh Sơn" />
                                    <p>
                                        Lê Thanh Sơn
                                        <small>0937843970</small>
                                        <small>lethanhson123@gmail.com</small>
                                    </p>
                                </li>
                                <li class="user-footer">
