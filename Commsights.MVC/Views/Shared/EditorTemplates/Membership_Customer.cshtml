@model Commsights.Data.Models.Membership
@(Html.Kendo().DropDownListFor(t => t)
                   .DataValueField("ID")
                   .DataTextField("Account")
                   .HtmlAttributes(new { style = "width:100%; font-size: 16px;" })
                    .Filter(FilterType.StartsWith)
                   .DataSource(dataSource => dataSource.Ajax().Read(t => t.Action("GetCustomerToList", "Membership")))
)