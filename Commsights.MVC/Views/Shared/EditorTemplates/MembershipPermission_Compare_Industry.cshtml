
@(Html.Kendo().DropDownListFor(t => t)
                   .DataValueField("IndustryID")
                   .DataTextField("IndustryName")
                   .HtmlAttributes(new { data_bind = ViewData["ValueBind"], @class = "form-control" })
                    .Filter(FilterType.StartsWith)
                   .DataSource(dataSource => dataSource.Ajax().Read(t => t.Action("GetDataTransferIndustryByMembershipIDAndIndustryToList", "MembershipPermission", new { membershipID = ViewData["CompareID"] })))
)